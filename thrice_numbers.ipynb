     "output_type": "execute_result"
    }
   ],
   "source": [
    "def sum_thrice(x, y, z):\n",
    "    sum = x + y + z \n",
    "\n",
    "    if x == y == z :\n",
    "      sum = sum * 3\n",
    "    return sum\n",
    "\n",
    "sum_thrice(3, 3, 3)    \n",
    "\n"
   ]
